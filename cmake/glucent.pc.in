prefix=@CMAKE_INSTALL_PREFIX@
exec_prefix=${prefix}
includedir=@CMAKE_INSTALL_FULL_INCLUDEDIR@
libdir=@CMAKE_INSTALL_FULL_LIBDIR@

Name: glucent
Description: A simple cross-platform OpenGL core profile loader and wrapper
Version: @GLUCENT_VERSION@
Requires.private: @GLUCENT_PKG_CONFIG_REQUIRES_PRIVATE@
Libs: -L${libdir} -l@GLUCENT_LIB_NAME@
Libs.private: @GLUCENT_PKG_CONFIG_LIBS_PRIVATE@
Cflags: -I${includedir}
